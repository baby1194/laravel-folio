{
    "name": "nonoesp/folio",
    "description": "Create webs to publish your content with custom templates.",
    "license": "MIT",
    "authors": [{
        "name": "Nono MartÃ­nez Alonso",
        "email": "mundowarezweb@gmail.com",
        "role": "Developer",
        "homepage": "https://nono.ma"
    }],
    "autoload": {
        "classmap": [
