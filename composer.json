{
    "name": "nonoesp/folio",
    "description": "Create webs to publish your content with custom templates.",
    "license": "MIT",
    "authors": [{
        "name": "Nono Mart√≠nez Alonso",
        "email": "mundowarezweb@gmail.com"
    }],
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/models",
            "src/facades"
        ],
        "psr-4": {
            "Nonoesp\\Folio\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "~5",
        "rtconner/laravel-tagging": "~2.0",
        "nonoesp/authenticate": "^5.5",
        "nonoesp/thinker": "*",
        "jenssegers/date": "^3.0",
        "roumen/feed": "2.12.*",
        "graham-campbell/markdown": "^10.0",
        "webuni/commonmark-attributes-extension": "^0.5.0",
        "laravelcollective/html": "5.5.*",
        "vinkla/hashids": "*",
        "intervention/image": "^2.3",
        "aws/aws-sdk-php": "~3.0",
        "spatie/laravel-newsletter": "^4.1",
        "spatie/laravel-feed": "^2.0",
        "doctrine/dbal": "^2.5",
        "mpociot/versionable": "~2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nonoesp\\Folio\\FolioServiceProvider"
            ],
            "aliases": {
                "Folio": "Nonoesp\\Folio\\Facades\\Folio",
                "Item": "Nonoesp\\Folio\\Models\\Item",
                "Property": "Nonoesp\\Folio\\Models\\Property",
                "Recipient": "Nonoesp\\Folio\\Models\\Recipient",
                "Subscriber": "Nonoesp\\Folio\\Models\\Subscriber"
            }
        }
    }
}