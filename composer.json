    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*",
        "rtconner/laravel-tagging": "^3.0",
        "nonoesp/authenticate": "^5.7",
        "nonoesp/thinker": "^1.1",
        "jenssegers/date": "^3.0",
        "laravelium/feed": "2.12.*",
        "graham-campbell/markdown": "^10.0",
        "webuni/commonmark-attributes-extension": "dev-master",
        "laravelcollective/html": "5.6.*",
        "vinkla/hashids": "^5.0.0",
        "intervention/image": "^2.3",
        "aws/aws-sdk-php": "~3.0",
        "spatie/laravel-newsletter": "^4.1",
        "spatie/laravel-feed": "^2.0",
        "doctrine/dbal": "^2.5",
        "mpociot/versionable": "~3.0",
        "michelf/php-markdown": "^1.8"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nonoesp\\Folio\\FolioServiceProvider"
            ],
            "aliases": {
                "Folio": "Nonoesp\\Folio\\Facades\\Folio",
                "Item": "Nonoesp\\Folio\\Models\\Item",
                "Property": "Nonoesp\\Folio\\Models\\Property",
                "Recipient": "Nonoesp\\Folio\\Models\\Recipient",
